<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
	<class name="model.Sensor" table="Sensor">

		<id name="id" type="java.lang.Long" column="id">
			<generator class="native"/>
		</id>

		<property name="name" column="name" type="java.lang.String"/>
		<property name="latitude" column="latitude" type="java.lang.Double"/>
		<property name="longitude" column="longitude" type="java.lang.Double"/>
		<property name="lastReading" column="lastreading" type="java.lang.Integer"/>

		<many-to-one name="district" column="district_id" class="model.location.District" not-null="true"/>
<!-- 	<set name="readings" inverse="true">
			<key column="sensorId"/>
			<one-to-many class="model.Reading"/>
		</set>
-->	</class>
</hibernate-mapping>